import random
n = ["قیچی","کاغذ","سنگ"]
x = random.choice(n)
y = input("سنگ یا کاغذ یا قیچی با تایپ کردن انتخاب خود را بنویسید:  ")
print(x)
if x ==y:
    print("مساوی شدید")
elif x=="کاغذ" and y=="قیچی" :
    print("آفرین شما بردید و کامپیوتر بازنده شد!")
elif x=="قیچی" and y=="کاغذ" :
    print("ای وای شما باختید و کامپیوتر برنده شد!")
elif x=="قیچی" and y=="سنگ" :
    print("آفرین شما برنده شدید")
elif x=="سنگ" and y=="قیچی" :
    print("متاسفم شما باختید")
elif x=="سنگ" and y=="کاغذ" :
    print("آفرین شما برنده شدید")
elif x == "کاغذ" and y == "سنگ":
    print("متاسفم شما باختید")
